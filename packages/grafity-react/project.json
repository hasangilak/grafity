{
  "name": "grafity-react",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/grafity-react/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/grafity-react",
        "main": "packages/grafity-react/src/index.ts",
        "tsConfig": "packages/grafity-react/tsconfig.lib.json",
        "assets": [
          "packages/grafity-react/*.md",
          {
            "input": "./packages/grafity-react/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./packages/grafity-react/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./packages/grafity-react",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./packages/grafity-react",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/grafity-react/jest.config.ts"
      }
    }
  }
}
