{
  "name": "grafity",
  "version": "2.0.0",
  "description": "Advanced React code analysis and visualization tool with comprehensive server infrastructure",
  "private": true,
  "scripts": {
    "build": "tsc && npm run build:client",
    "build:client": "cd client && npm run build",
    "build:server": "tsc",
    "start": "node dist/src/server/app.js",
    "start:server": "npm run build:server && npm run start",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "ts-node-dev --respawn --transpile-only src/server/app.ts",
    "dev:client": "cd client && npm start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist coverage .nyc_output",
    "demo:analyze": "ts-node scripts/demo/analyze.ts",
    "demo:visualize": "ts-node scripts/demo/visualize.ts",
    "demo:patterns": "ts-node scripts/demo/patterns.ts",
    "demo:graph-chat": "ts-node scripts/demo/graph-chat.ts",
    "demo:graph-chat-ui": "vite scripts/demo/graph-chat-ui.html --open",
    "docs:generate": "typedoc src --out docs/api",
    "docker:build": "docker build -t grafity:latest .",
    "docker:run": "docker run -p 3000:3000 grafity:latest",
    "migrate": "ts-node scripts/migrate.ts",
    "seed": "ts-node scripts/seed.ts",
    "graph:build": "npx ts-node tests/test-phase3-visualization.ts",
    "graph:visualize": "npx ts-node tests/test-phase3-visualization.ts",
    "graph:demo": "npm run graph:visualize",
    "neo4j:start": "./scripts/neo4j.sh start",
    "neo4j:stop": "./scripts/neo4j.sh stop",
    "neo4j:status": "./scripts/neo4j.sh status",
    "neo4j:shell": "./scripts/neo4j.sh shell",
    "neo4j:import": "./scripts/neo4j.sh import",
    "neo4j:clear": "./scripts/neo4j.sh clear",
    "neo4j:test": "npx ts-node src/core/graph-engine/test-neo4j.ts"
  },
  "keywords": [
    "react",
    "analysis",
    "visualization",
    "ast",
    "code-quality",
    "patterns",
    "hooks",
    "components",
    "server",
    "api",
    "graphql",
    "monitoring"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@graphql-tools/merge": "^9.1.1",
    "@graphql-tools/schema": "^10.0.25",
    "@nx/devkit": "21.5.3",
    "@types/node": "20.19.9",
    "@typescript-eslint/parser": "^7.16.0",
    "@typescript-eslint/typescript-estree": "^6.15.0",
    "apollo-server-express": "^3.13.0",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "chokidar": "^3.5.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.21.2",
    "express-rate-limit": "^8.1.0",
    "fast-glob": "^3.3.2",
    "glob": "^10.3.10",
    "graphql": "^16.11.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.20.0",
    "mongoose": "^8.18.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "mysql2": "^3.15.1",
    "neo4j-driver": "^5.28.2",
    "pg": "^8.16.3",
    "redis": "^5.8.2",
    "socket.io": "^4.8.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "typescript": "~5.9.2",
    "uuid": "^13.0.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@nx/eslint": "21.5.3",
    "@nx/eslint-plugin": "21.5.3",
    "@nx/jest": "21.5.3",
    "@nx/js": "21.5.3",
    "@nx/plugin": "^21.5.3",
    "@nx/workspace": "^21.5.3",
    "@playwright/test": "^1.55.1",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "^0.7.8",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "^10.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-util": "^29.7.0",
    "jsonc-eslint-parser": "^2.1.0",
    "nodemon": "^3.1.10",
    "nx": "21.5.3",
    "playwright": "^1.55.1",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.3.0"
  },
  "nx": {}
}
